//This program checks whether the number is prime or not 

#include <stdio.h>
#include <math.h>

int main() {
    int x, y;
    printf("Enter a number: ");
    scanf("%d", &x);
    
    y = ceil(sqrt(x));
    
    int z = x, count = 0;
    for(int i = 2; i <= y; i++) {
        if(z % i == 0)
            count = 1;
    }
    
    if((count == 0 && z != 1) || z == 2 || z == 3)
        printf("%d is a prime number.", z);
    else 
        printf("%d is not a prime number.", z);
    
    return 0;
}

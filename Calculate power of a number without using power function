#include <stdio.h>

int main() {

    int base, expo, result = 1;
    printf("enter the value of base: ");
    scanf("%d", &base);
    
    printf("enter the value of exponent: ");
    scanf("%d", &expo);
    
    while (expo != 0) {
        result *= base;
        expo--;
    }
    printf("The result is %d.", result);
    return 0;
}

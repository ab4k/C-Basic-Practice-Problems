//This program prints the smallest number of the array. 

#include <stdio.h>

int minArray(int arr[], int size) {
    int min;
    for(int i = 0; i < size; i++) {
        if(arr[i] < min) {
            min = arr[i];
        }
    }
    return min;
}

int main() {
    int array[] = {23, 56, 780, 89, 56, 45, 15, 24, 8};
    int minValue = minArray(array, 9);
    printf("The smallest number of the array is %d", minValue);
    
    return 0;
    
}

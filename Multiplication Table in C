//This is a program to print multiplication table of given number.

#include <stdio.h>

int main() {
  int num;
  printf("Enter the number of which the multiplication table will be printed: ");
  scanf("%d", &num);
  
  printf("The table of %d is: \n", num);
  for(int i = 1; i <= 10; i++) {
    printf("%d x %d = %d\n", num, i, num*i);
  }
  return 0;
}

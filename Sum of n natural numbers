//The below program takes the last integer (the value of n) as input and prints the sum of n natural numbers.

#include <stdio.h>

int main() {
    
    int n, sum = 0;
    printf("Enter the value of n: ");
    scanf("%d", &n);
    
    for(int i = 1; i <= n; i++) {
        sum += i;
    }
    printf("The sum of %d natural numbers is %d.", n, sum);
    
    return 0;
}

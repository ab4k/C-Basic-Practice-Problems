//Print the largest number of the array.

#include <stdio.h>

int returnMax(int array[], int size) {
    int max = 0;
    for (int i = 0; i < size; i++) {
        if (array[i] > max) {
            max = array[i];
        }
    }
    return max;
}

int main() {
    int arr[] = {80, 45, 6, 78, 90, 9};
    int max = returnMax(arr, 6);
    printf("The largest number in the array is %d", max);
    return 0;
}
